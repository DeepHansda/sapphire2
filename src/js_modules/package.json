{
  "name": "tailwindcss",
  "version": "1.0.0",
  "main": "tailwind.config.js",
  "scripts": {
    "build:boxiconsCSS": "cp -r ./node_modules/boxicons/css/boxicons.min.css ../static/css/boxicons.min.css",
    "build:boxiconsFonts": "cp -r ./node_modules/boxicons/fonts ../static/fonts",
    "build:htmx": "cp ./node_modules/htmx.org/dist/htmx.min.js ../static/js/htmx.min.js",
    "dev": "npx tailwindcss -i main.css -o ../static/css/main.css --watch",
    "build:daisyui": "cp ./node_modules/daisyui/dist/full.css ../static/css/daisyui.css",
    "build": "npm run build:boxiconsCSS && npm run build:boxiconsFonts && npm run build:htmx && build:daisyui"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@tailwindcss/typography": "^0.5.15",
    "boxicons": "^2.1.4",
    "daisyui": "^4.12.10",
    "htmx.org": "^1.9.12",
    "tailwindcss": "^3.4.12"
  }
}
